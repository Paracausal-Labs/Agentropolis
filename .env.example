# Groq LLM API key (get from https://console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# Deployer wallet private key (WITHOUT 0x prefix for PRIVATE_KEY, WITH 0x prefix for DEPLOYER_KEY)
PRIVATE_KEY=your_private_key_here
ADDRESS=0xYourWalletAddressHere
DEPLOYER_KEY=0xyour_deployer_key_here

# V4 Hook addresses (Base Sepolia) — update after deployment
NEXT_PUBLIC_COUNCIL_FEE_HOOK=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_SWAP_GUARD_HOOK=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_SENTIMENT_ORACLE_HOOK=0x0000000000000000000000000000000000000000

# Hook update API auth secret — generate with: openssl rand -hex 32
HOOK_AUTH_SECRET=CHANGE_THIS_TO_A_STRONG_RANDOM_SECRET
